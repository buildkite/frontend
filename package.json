    "react-router-relay": "https://github.com/sj26/react-router-relay/releases/download/v0.13.7%2Bclassic/react-router-relay-0.13.7.classic.tgz",
    "react-transition-group": "2.5.0",
    "search-query-parser": "1.3.0",
    "shuffle-array": "1.0.1",
    "styled-components": "3.4.10",
    "styled-transition-group": "1.0.0",
    "throttleit": "1.0.0",
    "uuid": "3.3.2",
    "whatwg-fetch": "3.0.0",
    "word-wrap": "1.2.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/app/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__generated__/"
    ],
    "testMatch": [
      "<rootDir>/app/**/__tests__/**/*.js",
      "<rootDir>/app/**/?(*.)(spec|test).js"
    ],
    "moduleDirectories": [
      "<rootDir>/node_modules",
      "<rootDir>/app",
      "<rootDir>"
    ]
  }
}
