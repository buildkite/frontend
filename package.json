    "eslint-plugin-flowtype": "^2.35.1",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-react": "^7.0.1",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "flow-bin": "^0.63.1",
    "http-server": "^0.11",
    "ignore-loader": "^0.1.2",
    "image-webpack-loader": "^3.0.0",
    "imports-loader": "^0.7.0",
