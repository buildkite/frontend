    "webpack-bundle-analyzer": "3.0.3",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.10",
    "winston": "3.1.0"
  },
  "dependencies": {
    "autosize": "4.0.2",
    "basscss": "8.0.10",
    "buffer-crc32": "0.2.13",
    "bugsnag-js": "4.7.3",
    "bugsnag-react": "1.1.1",
    "classnames": "2.2.6",
