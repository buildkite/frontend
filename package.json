    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-react": "7.7.0",
    "exports-loader": "0.7.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.6",
    "flow-bin": "0.69.0",
