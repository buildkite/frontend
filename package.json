    "moment": "^2.15.0",
    "object-assign": "^4.1.0",
    "prop-types": "^15.5.8",
    "pusher-js": "^4.0.0",
    "query-string": "^4.2.3",
    "raw-loader": "^0.5.1",
    "react": "^15.3.2",
    "react-addons-pure-render-mixin": "^15.3.2",
