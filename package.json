{
  "license": "MIT",
  "description": "Buildkite's frontend",
  "repository": {
    "type": "git",
    "url": "https://github.com/buildkite/frontend.git"
  },
  "scripts": {
    "env": "env",
    "start": "yarn run relay && yarn run webpack-dev-server",
    "test": "jest",
    "test-with-coverage": "jest --coverage",
    "lint": "eslint .",
    "lint-and-fix": "eslint --fix .",
    "flow": "./bin/flow-hook.sh",
    "typecheck": "yarn run flow",
    "build": "NODE_ENV=development webpack --config webpack/config.js --progress --bail",
    "build-production": "NODE_ENV=production webpack --config webpack/config.js --progress --bail",
    "bundle-analyze": "webpack --config webpack/config.js --profile --json > bundle-analysis/stats.json && webpack-bundle-analyzer bundle-analysis/stats.json dist --no-open --mode static -r bundle-analysis/bundle-analyzer-report.html",
    "webpack-dev-server": "NODE_ENV=development webpack-dev-server --config webpack/config.js --progress --colors --cache --inline --hot --host buildkite.dev --port 4890",
    "static-server": "node_modules/.bin/http-server -p 4890 -a buildkite.dev ./dist",
    "relay": "relay-compiler --schema app/graph/schema.json --src app"
  },
  "bin": {
    "flow": "./bin/flow-hook.sh"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.4.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-react-pure-components": "^2.2.2",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-relay": "^1.0.1",
    "babel-polyfill": "^6.13.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.3.13",
    "css-loader": "^0.28.3",
    "css-variables-loader": "^2.0.2",
    "eslint": "^3.17.1",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-flowtype": "^2.35.1",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-react": "^7.0.1",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "flow-bin": "^0.54.1",
    "http-server": "^0.10.0",
    "image-webpack-loader": "^3.0.0",
